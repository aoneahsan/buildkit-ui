{
  "name": "buildkit-ui",
  "version": "0.0.2",
  "description": "**BuildKit UI** is a Capacitor package that provides React UI components with comprehensive cross-platform tracking. It ensures every user interaction, error, and analytics event is captured with full platform context across Web, iOS, and Android by leveraging a powerful ecosystem of Capacitor packages.",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "android/gradle.properties",
    "android/settings.gradle",
    "android/proguard-rules.pro",
    "dist/",
    "ios/Plugin/",
    "BuildKitUi.podspec",
    "cli/"
  ],
  "author": {
    "email": "aoneahsan@gmail.com",
    "name": "Ahsan Mahmood (Aoneahsan)",
    "url": "https://aoneahsan.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aoneahsan/buildkit-ui.git"
  },
  "bugs": {
    "url": "https://github.com/aoneahsan/buildkit-ui/issues"
  },
  "keywords": [
    "capacitor",
    "build",
    "kit",
    "ui",
    "react",
    "components",
    "tracking",
    "analytics",
    "cross-platform",
    "mobile",
    "ios",
    "android",
    "web",
    "primereact",
    "tailwindcss",
    "accessibility",
    "i18n",
    "firebase"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:css && npm run tsc && rollup -c rollup.config.js",
    "build:css": "tailwindcss -i ./src/styles/input.css -o ./dist/styles/buildkit-ui.css --minify",
    "clean": "rimraf ./dist",
    "tsc": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint . --ext ts,tsx",
    "prettier": "prettier --write .",
    "prepublishOnly": "npm run build",
    "swiftlint": "cd ios && swiftlint lint --fix --format --path Plugin --verbose",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "bin": {
    "create-buildkit-app": "./cli/create-buildkit-app.js",
    "buildkit-ui": "./cli/buildkit-ui.js"
  },
  "dependencies": {
    "@capacitor/app": "^7.0.0",
    "@capacitor/device": "^7.0.0",
    "@capacitor/network": "^7.0.0",
    "@capacitor/preferences": "^7.0.0",
    "@vettabase/capacitor-auth-manager": "^1.0.0",
    "@vettabase/capacitor-biometric-authentication": "^1.0.0",
    "@vettabase/capacitor-firebase-kit": "^1.0.0",
    "@vettabase/capacitor-native-update": "^1.0.0",
    "@amplitude/analytics-browser": "^2.11.10",
    "@microsoft/clarity": "^0.8.18",
    "@sentry/react": "^8.48.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "i18next": "^24.2.1",
    "primereact": "^10.8.5",
    "primeicons": "^7.0.0",
    "react": "^18.3.1",
    "react-aria": "^3.37.2",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.1.0",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.1",
    "@capacitor/cli": "^7.4.1",
    "@capacitor/core": "^7.4.1",
    "@capacitor/ios": "^7.4.1",
    "@rollup/plugin-commonjs": "^29.1.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^13.0.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.15",
    "@types/node": "^24.0.12",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "autoprefixer": "^10.4.20",
    "chalk": "^5.3.0",
    "commander": "^13.0.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.38.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ora": "^8.1.1",
    "postcss": "^8.5.1",
    "prettier": "^3.6.2",
    "prompts": "^2.5.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.44.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@capacitor/core": "^7.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "release": {
    "npm": "https://www.npmjs.com/package/buildkit-ui"
  }
}
